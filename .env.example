# LLMResearch Configuration

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-3.5-turbo

# Custom LLM Provider Configuration
# Uncomment and modify these lines to use a custom LLM provider
# CUSTOM_LLM_NAME=my_custom_llm
# CUSTOM_LLM_API_KEY=your_custom_api_key_here
# CUSTOM_LLM_BASE_URL=https://api.example.com
# CUSTOM_LLM_MODEL=model_name

# Default LLM Provider (openai or custom)
DEFAULT_LLM_PROVIDER=openai

# Application Settings
# Maximum number of tokens to generate
# MAX_TOKENS=1000

# Temperature for sampling (0.0-1.0)
# TEMPERATURE=0.7

# Maximum number of reasoning steps
# MAX_REASONING_STEPS=5